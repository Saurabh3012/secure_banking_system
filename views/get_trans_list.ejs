<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src='https://www.google.com/recaptcha/api.js'></script>
</head>
<body>
<h1><%= title %></h1>
<p>

<table>
    <th>To</th>
    <th>Amount</th>
    <th>Status</th>
    <th>Datetime</th>

    <% if (trans){
    trans.forEach(function (item) {
    %>
        <tr>
            <td><%= item.to %></td>
            <td><%= item.amount %></td>
            <td><%= item.status %></td>
            <td><%= item.timestamp %></td>
        </tr>
    <% }) }%>
</table>

<form action="/get_trans_list/get_list" method="post">
    <input type="date" placeholder="From Date" name="from"  required />
    <input type="date" placeholder="To Date" name="to"  required />
    <input type="submit" value="Get Transactions" />
</form>
<a href="/banking">Back to Account Summary</a><br>
<a href="/logout">Logout</a>
</p>
</body>
</html>
